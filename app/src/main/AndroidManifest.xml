                <action android:name="android.intent.action.MAIN" />

                <category android:name="com.android.zlb.demos_SAMPLE_CODE" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gank.GankMainActivity"
            android:label="Gank-io">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.android.zlb.demos_SAMPLE_CODE" />
            </intent-filter>

        </activity>
        <activity
            android:name=".commens.list.sample.SampleActivity"
            android:label="SampleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.android.zlb.demos_SAMPLE_CODE" />
            </intent-filter>

        </activity>

        <activity android:name=".touchPager.BrowseModeActivity"

            />
